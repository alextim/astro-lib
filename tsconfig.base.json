{
  // "include": ["src", "__tests__"],
  "compilerOptions": {
    "module": "ES2020",
    "outDir": "./dist",
    "target": "ES2020",
    "declaration": true,
    // "emitDeclarationOnly": true,
    "strict": true,
    "moduleResolution": "node",
    "sourceMap": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "removeComments": true
  }
}
