{
  "vitest.enable": true,
  "search.exclude": {"**/*.log": true, "**/dist": true },
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.formatOnPaste": true,
  "editor.formatOnSave": true,
  "editor.formatOnSaveMode": "file",
  "editor.quickSuggestions": {
    "strings": true
  },
  "editor.codeActionsOnSave": {
    "source.fixAll": true
  },
  "cSpell.words": [
    "alextim",
    "Astro",
    "astrojs",
    "googlebot",
    "pnpm",
    "pnpx",
    "robotstxt",
    "vite",
    "changesets",
    "vitest",
    "outfile",
    "favicon",
    "favicons",
    "crossorigin",
    "anonymus",
    "<br/>",
    "frontmatter"

  ],
  "prettier.documentSelectors": ["**/*.ts"]
}
